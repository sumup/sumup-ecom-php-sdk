{
	"name": "sumup/sumup-ecom-php-sdk",
	"description": "SumUp PHP SDK",
	"type": "library",
	"license": "Apache-2.0",
	"version": "1.5.0",
	"keywords": [
		"sumup",
		"sdk",
		"payment processing",
		"ecommerce",
		"payment",
		"checkout"
	],
	"homepage": "https://developer.sumup.com",
	"authors": [
		{
			"name": "SumUp",
			"email": "integration@sumup.com",
			"homepage": "https://github.com/sumup"
		}
	],
	"scripts": {
		"cs": "php-cs-fixer fix --dry-run --diff",
		"test": "phpunit"
	},
	"require": {
		"php": "^5.6|^7.0|^8.0"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "3.5.0",
		"phpunit/phpunit": "^9.6"
	},
	"suggest": {
		"guzzlehttp/guzzle": "Allows for implementation of the Guzzle HTTP client"
	},
	"autoload": {
		"psr-4": {
			"SumUp\\": "src/SumUp/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SumUp\\Tests\\": "tests/"
		}
	}
}
